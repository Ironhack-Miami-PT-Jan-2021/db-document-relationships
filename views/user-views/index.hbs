<div class="user-card-container">

  {{#each users}}

  <div class="user-card">
    <a href="/users/{{this.id}}/posts">See Posts By This User</a>
    <h2>
    {{this.username}}
    </h2>

    <p>{{this.bio}}</p>

    <div>
      <a href="/posts/new/{{this.id}}">
      <img class="user-img" src="{{this.img}}" alt="">
      </a>
    </div>

  </div>

  {{/each}}
</div>